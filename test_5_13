//斐波那契数列
//1,1,2,3,5,8,13,21,34,...
//列出斐波那契数列的第n项

int Fib1(int n)
{
	if (n <= 2)
	{
		return 1;
	}
	else
	{
		return Fib1(n - 1) + Fib1(n - 2);
	}
}

int Fib2(int n)
{
	int a = 1;
	int b = 1;
	int c = 1;
	while (n > 2)
	{
		c = a + b;
		a = b;
		b = c;
		n--;
	}
	return c;
	
}
int main()
{
	int n = 0;
	scanf("%d", &n);
	int num = Fib2(n);
	printf("%d\n", num);
	return 0;
}
